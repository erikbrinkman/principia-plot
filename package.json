{
  "name": "principia-plot",
  "version": "0.0.12",
  "description": "A library for making plots based off of principae",
  "bin": "bin/cmd.js",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --forceExit --detectOpenHandles",
    "build": "tsc && node resources/gen_style.js > resources/princ_style.css",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "format": "prettier --write resources/gen_style.js 'src/*.ts' 'test/*.ts'",
    "docs": "typedoc --out docs src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikbrinkman/principia-plot.git"
  },
  "keywords": [
    "plot"
  ],
  "author": "Erik Brinkman <erik.brinkman@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikbrinkman/principia-plot/issues"
  },
  "homepage": "https://github.com/erikbrinkman/principia-plot#readme",
  "dependencies": {
    "clean-css": "^4.1.11",
    "d3": "^6.2.0",
    "jsdom": "^16.4.0",
    "yargs": "^16.1.1"
  },
  "devDependencies": {
    "@types/clean-css": "^4.2.2",
    "@types/d3": "^6.2.0",
    "@types/jest": "^26.0.15",
    "@types/jsdom": "16.2.5",
    "@types/node": "^14.14.10",
    "@types/yargs": "^15.0.10",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "preset": "ts-jest",
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  }
}
