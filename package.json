{
  "name": "principia-plot",
  "version": "0.0.6",
  "description": "A library for making plots based off of principae",
  "bin": "bin/cmd.js",
  "scripts": {
    "test": "jest --forceExit",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "docs": "typedoc --out docs src",
    "style": "cp resources/base_style.css resources/princ_style.css && jq -rf resources/themes.jq resources/themes.json >> resources/princ_style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikbrinkman/principia-plot.git"
  },
  "keywords": [
    "plot"
  ],
  "author": "Erik Brinkman <erik.brinkman@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikbrinkman/principia-plot/issues"
  },
  "homepage": "https://github.com/erikbrinkman/principia-plot#readme",
  "dependencies": {
    "d3": "^4.12.0",
    "jsdom": "^11.6.2",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@types/d3": "^4.12.0",
    "@types/jsdom": "^11.0.4",
    "@types/node": "^8.5.10",
    "@types/yargs": "^10.0.1",
    "jest": "^21.2.1",
    "ts-jest": "^21.2.4",
    "tslint": "^5.9.1",
    "typedoc": "^0.10.0",
    "typescript": "^2.7.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  }
}
